{
	"name": "xiaobi",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"dev": "npm run start",
		"start": "cross-env NODE_ENV=development webpack --config=build/webpack.dev.js --progress",
		"build:repo": "cross-env NODE_ENV=production webpack --config=build/webpack.prod.js --progress",
		"build:zip": "zip -r dist.zip dist",
		"build": "run-s build:repo build:zip",
		"lint": "npm run lint-js && npm run lint-style",
		"lint-js": "eslint --config=.eslintrc.js --ext .ts,.tsx,.js src",
		"lint-style": "stylelint --config=.stylelintrc.js src/**/*.{less,css,scss}"
	},
	"keywords": [],
	"author": "ElegantYU",
	"license": "ISC",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.{ts,tsx,js}": [
			"eslint --fix"
		],
		"*.{css,scss,less}": [
			"stylelint --config .stylelintrc.js"
		],
		"*.{ts,tsx,js,json,html,yml,css,scss,less,md}": [
			"prettier --write"
		]
	},
